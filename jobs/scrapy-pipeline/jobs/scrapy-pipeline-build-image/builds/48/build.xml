                    env.DOCKER_IMG = &quot;$SPIDER_USER/$SPIDER_NAME:$SPIDER_VERSION&quot;.replace(&apos;.&apos;, &apos;-&apos;).toLowerCase()
                    env.PLAYWRIGHT_ENABLED=params.PLAYWRIGHT_ENABLED ? params.PLAYWRIGHT_ENABLED : false
                    env.PLAYWRIGHT_BROWSER=params.PLAYWRIGHT_BROWSER
                }
            }
        }
        stage(&apos;Pull Repo&apos;) {
            steps {
                sh &apos;rm -rf repo&apos;
                sh &quot;mkdir repo || true&quot;
                dir(&quot;repo&quot;) {
                checkout([
                    $class: &apos;GitSCM&apos;, 
                    branches: [[name: &quot;*/master&quot;]],
                    userRemoteConfigs: [[
                        url: &quot;$GIT_URL&quot;,
                        credentialsId: env.GIT_CREDENTIALS_ID
                            ]]
                        ])
                    }
                }
            }
        stage(&apos;Change settings into override, move repo settings into build&apos;) {
            steps {
                sh &apos;mv generic/generic/settings.py generic/generic/settings_override.py&apos;
                sh &apos;find ./repo -name settings.py -exec cp {} ./generic/generic/settings.py \\;&apos;
                sh &apos;./settings_override.sh&apos;
                sh &apos;find ./repo -name middlewares.py -exec cat {} \\; &gt;&gt; ./generic/generic/middlewares.py&apos;
            }
        }
        stage (&apos;Copy Spiders into build&apos;) {
            steps {
                script {
                    sh &apos;&apos;&apos;
                    find ./repo -type f -path &apos;*/spiders/*.py&apos; -exec cp {} ./generic/generic/spiders/ \\;
                    &apos;&apos;&apos;
                }
            }
        }
        stage (&apos;Docker Build&apos;) {
            steps {
                script{
                    sh &quot;docker build -t $DOCKER_IMG --build-arg PLAYWRIGHT_ENABLED=$PLAYWRIGHT_ENABLED --build-arg PLAYWRIGHT_HEADLESS=$PLAYWRIGHT_HEADLESS --build-arg PLAYWRIGHT_BROWSER=$PLAYWRIGHT_BROWSER --build-arg SPIDER_NAME=$SPIDER_NAME .&quot;
                }
            }
        }
        stage (&apos;Push&apos;) {
            steps {
                script {
                    docker.withRegistry(&apos;&apos;, &apos;docker-credentials&apos;) {
                        echo &apos;Logged in to docker...&apos;
                        def dockerImage = docker.image(&quot;$DOCKER_IMG&quot;)
                        dockerImage.push()
                    }
                }
            }
        }
        }
    }
</script>

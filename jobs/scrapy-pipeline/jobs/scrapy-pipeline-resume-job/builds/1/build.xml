                    sh &quot;kubectl get ns&quot;
                    sh &quot;kubectl delete job scrapy-job-$NAMESPACE || true&quot;
                    script {
                        sh &quot;&quot;&quot;
                            ISRUNNING=&apos;&apos;;
                            ATTEMPTS=20;
                            while [ -z &quot;\$ISRUNNING&quot; ] &amp;&amp; [ \$ATTEMPTS -gt 0 ]; 
